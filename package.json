{
  "name": "create-tgen",
  "version": "0.1.0",
  "description": "A CLI tool to quickly scaffold new projects using customizable templates.",
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "author": {
    "name": "Matías Castañeda <@matcastaneda>",
    "url": "https://matcastaneda.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matcastaneda/create-tgen.git",
    "directory": "create-tgen"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "next.js",
    "react",
    "vite",
    "template",
    "generator"
  ],
  "type": "module",
  "bin": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "prebuild": "rm -rf dist",
    "build": "tsup",
    "start": "node dist/index.js",
    "lint": "pnpm run --parallel --stream lint:format && pnpm run lint:typecheck",
    "lint:typecheck": "tsc --noEmit",
    "lint:check": "prettier --check \"**/*.ts\" --cache",
    "lint:format": "prettier --write \"**/*.ts\" --cache",
    "prepare": "husky",
    "release": "changeset version",
    "pub:release": "pnpm build && pnpm publish --access public"
  },
  "dependencies": {
    "commander": "12.1.0",
    "execa": "9.4.1",
    "fast-glob": "3.3.2",
    "ora": "8.1.0",
    "picocolors": "1.1.1",
    "prompts": "2.4.2",
    "type-fest": "4.26.1",
    "validate-npm-package-name": "6.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.9",
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@ianvs/prettier-plugin-sort-imports": "4.3.1",
    "@microsoft/api-extractor": "7.47.11",
    "@types/node": "22.7.8",
    "@types/prompts": "2.4.9",
    "@types/validate-npm-package-name": "4.0.2",
    "husky": "9.1.6",
    "prettier": "3.3.3",
    "pretty-quick": "4.0.0",
    "tsup": "8.3.0",
    "typescript": "5.4.2",
    "validate-npm-package-name": "6.0.0"
  },
  "packageManager": "pnpm@9.12.2"
}
